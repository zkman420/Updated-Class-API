{
    "manifest_version": 3,
    "name": "Persistent Log Panel",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "scripting", "alarms", "cookies"],  
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }